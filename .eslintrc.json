{
  "rules": {
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": [
      0
    ],
    "curly": [
      2,
      "all"
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-infix-ops": 2,
    "semi": [
      2,
      "always"
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "no-with": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
